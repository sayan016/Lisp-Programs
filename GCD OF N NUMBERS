(defun my_gcd (a b)
(loop
	(if(> a b)
	 (setq a (- a b)))
	(if(> b a)
	(setq b (- b a)))
	 (when (= a b)(return a)))
	 (return-from my_gcd a)
	 )
(princ "Enter number of elements :- ")
(setq n (read))
(princ "Enter the numbers: ")
(setq a (read))
(setq b (read))
 (setq c (my_gcd a b))
 (loop for x from 3 to n
	do (setq p (read))  
	(setq d (my_gcd c p))
		(setq c d))
 (format t "The GCD of the numbers is ~D" c)
